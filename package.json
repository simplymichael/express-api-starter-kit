{
  "name": "express-api-starter-kit",
  "version": "1.0.0",
  "description": "A starter kit for building REST API applications with Express.js.",
  "main": "index.js",
  "scripts": {
    "test": "cross-env NODE_ENV=test mocha __tests__/\"{,/**/}*.test.js\"",
    "test:coverage": "nyc npm test",
    "build": "npm install",
    "serve": "node ./src/index.js",
    "serve:watch": "nodemon ./src/index.js",
    "commit": "git-cz",
    "lint": "npx eslint src/**/*.js __tests__/**/*.js",
    "lint:fix": "npm run lint -- --fix"
  },
  "keywords": [
    "api",
    "express",
    "kit",
    "rest",
    "rest-api",
    "starter",
    "setup"
  ],
  "author": "simplymichael <simplymichaelorji@gmail.com> (https://github.com/simplymichael)",
  "license": "MIT",
  "dependencies": {
    "awilix": "10.0.1",
    "bcrypt": "5.1.1",
    "connect-redis": "7.1.1",
    "cookie-parser": "1.4.6",
    "debug": "4.3.4",
    "dotenv": "16.4.5",
    "email-validator": "2.0.4",
    "express": "4.19.2",
    "express-validator": "7.0.1",
    "http-errors": "2.0.0",
    "jsonwebtoken": "9.0.2",
    "mongoose": "8.2.3",
    "node-cache": "5.1.2",
    "redis": "4.6.13",
    "serialijse": "0.3.0",
    "winston": "3.13.0",
    "winston-daily-rotate-file": "5.0.0"
  },
  "devDependencies": {
    "chai": "5.1.0",
    "commitizen": "4.3.0",
    "cross-env": "7.0.3",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.57.0",
    "ghooks": "2.0.4",
    "mocha": "10.3.0",
    "nodemon": "3.1.0",
    "nyc": "15.1.0",
    "superagent": "8.1.2",
    "superagent-prefix": "0.0.2"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "pre-commit": "npm run lint && npm run test:coverage"
    }
  },
  "engines": {
    "node": ">=18.18.2",
    "npm": ">=9.8.1"
  }
}
